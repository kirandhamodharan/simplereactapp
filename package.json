  "description": "Template for a simple react application with no CRA",
  "scripts": {
    "build": "webpack --config ./webpack-config/prod.js --progress --colors",
    "dev": "webpack-dev-server --config ./webpack-config/dev.js --color",
    "start": "serve ./build"
  },
  "keywords": [
    "react",
    "template"
  ],
  "author": "Kiran Dhamodharan <kirandhamodharan@gmail.com>",
  "license": "ISC",
